{
  "name": "cb2fa",
  "version": "2.0.0",
  "description": "CB2FA - Community-Based Two-Factor Authentication v2.0.0 - Matrix Only. Company: EasyProTech LLC (www.easypro.tech). Dev: Brabus. Created: 2025-08-10 21:40 MSK",
  "compilerOptions": {
    "allowJs": true,
    "lib": ["deno.window"],
    "strict": false,
    "checkJs": false,
    "noImplicitAny": false,
    "types": ["deno"]
  },
  "tasks": {
    "start": "deno run --allow-net --allow-env --allow-read src/main.ts",
    "middleware": "deno run --allow-net --allow-env --allow-read middleware.ts",
    "dev": "deno run --allow-net --allow-env --allow-read --watch src/main.ts"
  }
}
